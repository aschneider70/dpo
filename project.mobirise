n    }\n\n    &.disabled,\n    &:disabled {\n        color: @active-color !important;\n        background-color: @active-background !important;\n        border-color: @active-background !important;\n    }\n}\n\n// .button-variant(@background) {\n//     border: 0px!important;\n//     &:hover, &:focus {\n//         background-color: @secondaryColor!important;\n//         box-shadow: none!important;\n//     };\n// }\n\n// .button-outline-variant(@color) {\n// border: 0px!important;\n// background-color: white!important;\n// box-shadow: 0 1px 2px 0 rgba(0,0,0,.20);\n// &:hover, &:focus {\n//     background-color: white!important;\n//     color: @primaryColor!important;\n//     box-shadow: 0 1px 5px 1px rgba(0, 0, 0, 0.2)\n// }\n// }\n\n.plan-variant(@className, @color) {\n    &@{className} {\n        .mbr-plan-subtitle,\n        .mbr-plan-price-desc {\n            @max: max(red(@color), green(@color), blue(@color));\n\n            & when (@max > 200) {\n                color: lighten(@color, 40%);\n            }\n\n            & when (@max > 85) and (@max <=200) {\n                color: lighten(@color, 25%);\n            }\n\n            & when (@max > 50) and (@max <=85) {\n                color: lighten(@color, 45%);\n            }\n\n            & when (@max <=50) {\n                color: lighten(@color, 70%);\n            }\n        }\n    }\n}\n\n.button-size(@btn-padding-y, @btn-padding-x, @btn-font-size, @btn-border-radius) {\n    padding: @btn-padding-y @btn-padding-x;\n    border-radius: @btn-border-radius;\n}\n\n.btn {\n    .button-size(\n        @btn-padding-y,\n        @btn-padding-x,\n        @btn-font-size,\n        @btn-border-radius\n    );\n}\n\n.btn-sm {\n    .button-size(\n        @btn-padding-y-sm,\n        @btn-padding-x-sm,\n        @btn-font-size-sm,\n        @btn-border-radius-sm\n    );\n}\n\n.btn-md {\n    .button-size(\n        @btn-padding-y-md,\n        @btn-padding-x-md,\n        @btn-font-size-md,\n        @btn-border-radius-md\n    );\n